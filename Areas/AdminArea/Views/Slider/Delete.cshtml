@model Slider

@{
    ViewData["Title"] = "Delete";
}

<div class="container">
    <div class="row">
        <div class="col-6">
            <img src="~/Main-Images/@Model.ImageUrl" />
        </div>

        <div class="col-6">
            <a asp-controller="Slider" asp-action="Index" class="btn btn-warning" style="color:white">Go Back</a>
        </div>
        <form class="forms-sample mt-3" method="Post">
             <button class="btn btn-danger">Delete</button>
        </form>
    </div>
</div>

